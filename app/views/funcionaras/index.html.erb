<p id="notice"><%= notice %></p>

<h1>Funcionaras</h1>

<table>
  <thead>
    <tr>
      <th>Info</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @funcionaras.each do |funcionara| %>
      <tr>
        <td><%= funcionara.info %></td>
        <td><%= funcionara.name %></td>
        <td><%= link_to 'Show', funcionara %></td>
        <td><%= link_to 'Edit', edit_funcionara_path(funcionara) %></td>
        <td><%= link_to 'Destroy', funcionara, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Funcionara', new_funcionara_path %>
