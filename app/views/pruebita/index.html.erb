<p id="notice"><%= notice %></p>

<h1>Pruebita</h1>

<table>
  <thead>
    <tr>
      <th>Info</th>
      <th>Name</th>
      <th>Age</th>
      <th>Salary</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pruebita.each do |pruebitum| %>
      <tr>
        <td><%= pruebitum.info %></td>
        <td><%= pruebitum.name %></td>
        <td><%= pruebitum.age %></td>
        <td><%= pruebitum.salary %></td>
        <td><%= link_to 'Show', pruebitum %></td>
        <td><%= link_to 'Edit', edit_pruebitum_path(pruebitum) %></td>
        <td><%= link_to 'Destroy', pruebitum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pruebitum', new_pruebitum_path %>
